{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "off"
    ],
    "react/jsx-props-no-spreading": [
      "off"
    ],
    "react/forbid-prop-types": [
      "off"
    ],
    "react/no-unused-prop-types": [
      "off"
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "src"
        ]
      }
    }
  }
}